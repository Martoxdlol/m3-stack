{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist/server/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/public/$1"
        },
        {
            "src": "^(?!/api/)(.*)",
            "dest": "/dist/public/index.html"
        }
    ],
    "builds": [
        {
            "src": "dist/server/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/public/**/*",
            "use": "@vercel/static"
        }
    ]
}
